[*.d]
end_of_line=lf
insert_final_newline=true
charset=UTF-8
indent_style=space
indent_size=2
trim_trailing_whitespace=true
max_line_length=80
dfmt_brace_style=otbs
dfmt_soft_max_line_length=70
dfmt_align_switch_statements=true
dfmt_keep_line_breaks=true
dfmt_space_before_aa_colon=true
dfmt_single_template_constraint_indent=true
dfmt_outdent_attributes=true
dfmt_split_operator_at_line_end=true
dfmt_space_after_cast=true
dfmt_space_after_keywords=true
dfmt_space_before_function_parameters=false
dfmt_selective_import_space=true
dfmt_compact_labeled_statements=true
